// Single Inheritance

class Animal{
    void eat() {
        System.out.println("Eating....");
    }
    void states() {
        System.out.println("Animals has name...");
        System.out.println("Animals has color...");
    }
}
class Dog extends Animal {
    void bark() {
        System.out.println("Barking...");
    }
    void wagTail() {
        System.out.println("Wagging the tail...");
    }
}
class SingleInheritance {
    public static void main(String [] args) {
        Dog d = new Dog();
        d.eat();
        d.states();
        d.bark();
        d.wagTail();
    }
}
